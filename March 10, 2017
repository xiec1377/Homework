%Extra Loop Practise Question 2

var even1 : int 
var even2 : int

put "Give me two even integers. " ..
get even1, even2 
if even1<even2 then 
    for i : even1 .. even2 by 2
        put i, ", " .. 
    end for 
elsif even2<even1 then 
    for i : even2 .. even1 by 2 
        put i, ", " .. 
    end for 
else
    put "You suck." 
end if 
